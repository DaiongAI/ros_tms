<?xml version="1.0"?>
<launch>
  <!-- Make skeleton streams -->
  <node name="skeleton_integrator" pkg="tms_ss_kinect_v2" args="1" type="skeleton_integrator"/>

  <!-- For visualize skeleton in rViz -->
  <param name="robot_description" textfile="$(find tms_ss_kinect_v2)/urdf/skeleton.urdf"/>
  <node name="skeleton_state_publisher" pkg="tms_ss_kinect_v2" type="skeleton_state_publisher"/>
  <!--<node name="rviz" pkg="rviz" type="rviz" respawn="true"/>-->
  <node name="rviz" pkg="rviz" type="rviz" respawn="true"/>
</launch>
