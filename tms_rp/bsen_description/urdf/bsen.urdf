  <!-- ========== robots ========== -->  

  <!-- ========== environment ========== -->

  <material name="temp">
    <color rgba="0.5 0.5 0.8 1.0"/>
  </material>

  <material name="beige">
    <color rgba="0.98 0.915 0.72 1.0"/>
  </material>

  <joint name="world_joint" type="fixed">
    <parent link="origin_position"/>
    <child link="bsen_room"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="bsen_room">
    <visual>
      <geometry>
        <mesh filename="package://environmental_model/model/bsen.stl"/>
      </geometry>
      <material name="temp"/>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
  </link>

  <joint name="bsen_origin_joint" type="fixed">
    <parent link="bsen_room"/>
    <child link="bsen_origin"/>
    <origin xyz="0.0 0 0" rpy="0 0 0"/>
  </joint>

  <link name="bsen_origin">
    <visual>
      <geometry>
        <box size="0.1 0.1 0.1"/>
      </geometry>
      <origin xyz="0 0 0" rpy="0 0 0"/>
    </visual>
  </link>

  <joint name="bsen_room_to_chair" type="fixed">
    <parent link="bsen_origin"/>
    <child link="chair"/>
    <origin xyz="0 0 0"/>
  </joint>

  <link name="chair">
    <visual>
      <geometry>
        <mesh filename="package://environmental_model/model/chair.stl"/>
      </geometry>
      <material name="temp"/>
      <origin xyz="12.3 5.62 0.51" rpy="0 0 1.57079633"/>
    </visual>
  </link>

